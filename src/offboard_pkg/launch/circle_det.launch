<launch>

	<!-- run the circle_det -->
    <node pkg="offboard_pkg" type="ellipse_det" name="ellipse_det" output="screen">
        <param name="camera_topic" type="string" value="/d400/color/image_raw" />
        <param name="camera_info" type="string" value="$(find offboard_pkg)/config/camera_param.yaml" />
        <param name="with_training" type="bool" value="false" />
        <param name="train_imlist" type="string" value="$(find offboard_pkg)/dataset/ellipse/landing_patches.txt" />
        <param name="train_imdir" type="string" value="$(find offboard_pkg)/dataset/ellipse/landing_patches" />
        <param name="saving_center" type="bool" value="false" />
        <param name="saving_path" type="string" value="$(find offboard_pkg)/dataset/ellipse/images_from_camera" />
    </node>

</launch>

